{
  "name": "money-book-api",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "src/types/*.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist/ && mkdir dist && cp -r src/openapi dist",
    "pretest": "node set-version.js",
    "test": "jest --runInBand",
    "dev": "export TZ=Asia/Hong_Kong && node set-version.js && ts-node-dev --clear --respawn --exit-child -r tsconfig-paths/register src/",
    "prebuild": "node set-version.js",
    "build": "yarn clean && tsc -p tsconfig.json",
    "start": "node -r module-alias/register dist/index.js"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "fastify": "^3.28.0",
    "fastify-cors": "^6.0.3",
    "js-yaml": "^4.1.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mongoose": "^6.9.1",
    "mongoose-plugin-autoinc": "^1.1.9",
    "tmp": "^0.2.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "expect": "^29.4.2",
    "git-rev-sync": "^3.0.2",
    "jest": "^29.4.2",
    "mongodb-memory-server": "^8.11.4",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
